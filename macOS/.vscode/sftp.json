{
    "protocol": "sftp",
    "host": "your.host",
    "port": 22,
    "username": "yourname",
    "remotePath": "/path/to/remote",
    "privateKeyPath": "/path/to/key",
    "passphrase": true,
    "uploadOnSave": false,
    "downloadOnOpen": false,
    "watcher": {
        "files": "**/*",
        "autoUpload": true,
        "autoDelete": true
    },
    "algorithms": {
        "serverHostKey": [
            "ssh-rsa",
            "ssh-dss",
            "ecdsa-sha2-nistp256",
            "ecdsa-sha2-nistp384",
            "ecdsa-sha2-nistp521"
        ]
    },
    "ignore": [
        ".vscode",
        ".git",
        ".DS_Store",
        "vendor"
    ]
}
