# ssh
IdentityFile      ~/.ssh/id_rsa
ForwardAgent      yes
HostKeyAlgorithms +ssh-dss

# skip passphrase
UseKeyChain    yes
AddKeysToAgent yes

# host config
Host github.com
  User         git
  Port         22
  Hostname     github.com
  # IdentityFile /path/to/id_rsa
  IdentityOnly yes

